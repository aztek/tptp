fof(majority,axiom,(
    ! [X,Y] : f(X,X,Y) = X )).

fof(permute1,axiom,(
    ! [X,Y,Z] : f(X,Y,Z) = f(Z,X,Y) )).

fof(permute2,axiom,(
    ! [X,Y,Z] : f(X,Y,Z) = f(X,Z,Y) )).

fof(associativity,axiom,(
    ! [W,X,Y,Z] : f(f(X,W,Y),W,Z) = f(X,W,f(Y,W,Z)) )).

